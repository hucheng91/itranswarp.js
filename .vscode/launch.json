{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach",
            "port": 9229
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/app.js"
        },
        {
            "name": "Start",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/www/start.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}/www",
            "preLaunchTask": null,
            "runtimeExecutable": null,
    
            "env": {
                "NODE_ENV": "development"
            },
            "console": "internalConsole",
            "sourceMaps": false,
            "outDir": null
        }
    ]
}